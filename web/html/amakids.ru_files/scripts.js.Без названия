function adaptive(){if($(window).width()<992){var $responsiveMenu=$(".responsive-menu"),animating=!1;if(!$responsiveMenu.length)
    $responsiveMenu=$("<div class='responsive-menu'></div>").appendTo("header .header-center .container").append($(".header-main-nav").show(),$(".advance-menu").show());$(".innerList").off('click').click(function(e){$(this).find(".inner-list").slideToggle(300)});$(".hamburger").click(function(){var thismenu=$(this);if($responsiveMenu.width()===0){$responsiveMenu.animate({width:320},function(){$responsiveMenu.addClass('opened_menu');thismenu.html("<i class='fa fa-times'></i>")})}
    if($responsiveMenu.hasClass('opened_menu')){$responsiveMenu.animate({width:0},function(){$responsiveMenu.removeClass('opened_menu');thismenu.html("<i class='fa fa-bars'></i>")})}})}
    if($(window).width()<992){$(".header-language>a").click(function(e){e.preventDefault()});$(".header-language").click(function(e){$(".header-language .inner-list").toggle(200)})}}(function($){$(function(){if($('main').find('.error-container').length>0){$('main').addClass('page-404')}
    $('.blog-slider-item').each(function(){if($(this).find('h3').text().length>50){var str=$(this).find('h3').text();var i=48;while(i<=str.length){i++;if(str[i]==' '){break}}
        $(this).find('h3').text($(this).find('h3').text().substring(0,i)+'...')}});$(".aside-top").click(function(){$("html, body").animate({scrollTop:0},"slow");return!1});function getUrlVars(href){var vars={};var parts=href.replace(/[?&]+([^=&]+)=([^&]*)/gi,function(m,key,value){vars[key]=value});return vars}
    $(".video-item").each(function(){var videoId=getUrlVars($(this).attr("href")).v}).click(function(e){e.preventDefault();var videoId=getUrlVars($(this).attr("href")).v,$videoPopup=$("<div class='popup-wrapper'></div>").appendTo("body").html('<iframe width="640" height="360" src="https://www.youtube.com/embed/'+videoId+'?rel=0&autoplay=1" frameborder="0" allowfullscreen></iframe>').fadeIn(500);$videoPopup.click(function(e){if(e.target===this){var $this=$(this);$this.fadeOut(500).delay(500);setTimeout(function(){$this.remove()},500)}})});var $thanks=$(".thanks-popup");$(".popup-wrapper").click(function(e){if(e.target===this){$(this).fadeOut(500);$thanks.fadeOut(500);$('.white-over').removeClass('white-over')}});$("label input, label textarea").keydown(function(){$(this).parent("label").removeClass('validate')});$(".leave-review").click(function(){$(".reviews-popup").fadeIn(500);$(".reviews-form-wrapper:not(.thanks-popup)").fadeIn(500)});$(".single-list>li").click(function(e){var $glyphicon=$(this).children(".glyphicon");$(this).children(".single-inner-list").slideToggle(300);if($glyphicon.hasClass("glyphicon-triangle-bottom"))$glyphicon.removeClass("glyphicon-triangle-bottom").addClass("glyphicon-triangle-top");else $glyphicon.removeClass("glyphicon-triangle-top").addClass("glyphicon-triangle-bottom")});$(".speed-faq-caption").click(function(){var $arrow=$(this).find(".fa");$(".speed-faq-caption").not($(this)).siblings(".faq-answer").slideUp(300).end().find(".fa").addClass("fa-angle-down");$(this).siblings(".faq-answer").slideToggle(300);if($arrow.hasClass('fa-angle-down')){$arrow.removeClass("fa-angle-down").addClass("fa-angle-up")}
    else $arrow.removeClass("fa-angle-up").addClass("fa-angle-down")});$(".hide-answer").click(function(e){e.preventDefault();$(this).parents('.faq-answer').slideUp(300).siblings(".speed-faq-caption").find(".fa").addClass("fa-angle-down")});$('.fragment-item').each(function(){$(this).find('img:first-child').clone().appendTo('.fragments-thumbs')});$('.fragments-thumbs img:first-child').addClass('active');$('[data-scroll-to]').click(function(e){e.preventDefault();var $scrollTarget=$($(this).attr('data-scroll-to'));if($scrollTarget.length!==0){$('html, body').animate({scrollTop:$scrollTarget.offset().top},500)}});$('.speed-faq-bottom .button').click(function(e){e.preventDefault();$('.speed-faq-item-hidden').slideDown(300);$(this).fadeOut(200)});adaptive();$(window).resize(adaptive);function winnersRender(){$('.groups-list li').each(function(){if($(this).hasClass('active'))$($(this).data('target')).removeClass('hidden');else $($(this).data('target')).addClass('hidden')})}
    winnersRender();$('.groups-list a').click(function(e){e.preventDefault()});$('.groups-list li').click(function(){$(this).addClass('active').siblings('li').removeClass('active');winnersRender()})})})(jQuery)